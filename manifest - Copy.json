{
	"manifest_version": 2,
	"name": "Traversy Launcher",
	"description": "Quick launch Traversy Media",
	"version": "1.0.0",
	"icons": {"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab"]
}
{
"manifest_version": 2,
    "name": "Profanity Filter",
    "description": "Helps censor undesirable words to make the internet family friendly for the kids",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}